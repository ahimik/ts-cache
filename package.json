{
    "name": "ts-cache",
    "version": "0.0.1-beta.1",
    "main": "./dist/index.js",
    "module": "./dist/esm5/index.js",
    "es2015": "./dist/esm2015/index.js",
    "types": "./dist/index.d.ts",
    "description": "Observable/Promise Cache Decorator",
    "license": "MIT",
    "author": "Aliaksandr Himik",
    "repository": {
        "type": "git",
        "url": "https://github.com/ahimik/ts-cache.git"
    },
    "homepage": "https://github.com/ahimik/ts-cache#readme",
    "bugs": {
        "url": "https://github.com/ahimik/ts-cache/issues"
    },
    "keywords": [
        "cache",
        "caching",
        "cacheable",
        "observable",
        "promise",
        "decorator",
        "typescript",
        "memory",
        "local-storage"
    ],
    "scripts": {
        "test": "karma start",
        "lint": "npx eslint src",
        "build:cjs": "tsc",
        "build:esm5": "tsc --p tsconfig.esm5.json",
